{
  "name": "@findaspring/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "build": "echo 'No build step yet'",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" \"scripts/**/*.js\"",
    "typecheck": "find src test scripts -name '*.js' -exec node --check {} +",
    "seed:moderator": "node scripts/seed_moderator.js",
    "db:migrate": "node scripts/migrate.js",
    "test": "node --test",
    "test:unit": "node --test src/lib/passwords.test.js src/lib/tokens.test.js",
    "test:integration": "node --test --test-concurrency=1 test/*.integration.test.js",
    "test:smoke": "node scripts/smoke_test.js"
  },
  "dependencies": {
    "pg": "^8.13.1",
    "ws": "^8.18.0"
  }
}
